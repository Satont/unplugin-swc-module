{
  "name": "unplugin-swc",
  "version": "0.0.0",
  "description": "SWC plugin for Vite and Rollup",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --shims",
    "test": "npm run build && uvu -r esbuild-register --ignore fixtures",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "unplugin": "^0.9.6",
    "defu": "^6.1.0",
    "load-tsconfig": "^0.2.3",
    "@rollup/pluginutils": "^4.2.1",
    "path-exists": "^5.0.0"
  },
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@swc/core": "^1.3.2",
    "@types/node": "^18.7.18",
    "esbuild": "0.15.9",
    "esbuild-register": "^3.3.3",
    "prettier": "2.7.1",
    "rollup": "^2.79.1",
    "tsup": "6.2.2",
    "typescript": "4.8.3",
    "uvu": "^0.5.6",
    "vite": "^3.1.3"
  },
  "peerDependencies": {
    "@swc/core": ">=1.3.2"
  }
}